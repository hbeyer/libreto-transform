<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="collection">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="metadata"/>
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="item"/>
          <xs:element ref="miscellany"/>
          <xs:element ref="section"/>
          <xs:element ref="pb"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="metadata">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="heading"/>
        <xs:element ref="owner"/>
        <xs:element ref="fileName"/>
        <xs:element ref="description"/>
        <xs:element ref="geoBrowserStorageID"/>
        <xs:element maxOccurs="unbounded" ref="creatorReconstruction"/>
        <xs:element ref="yearCollection"/>
        <xs:element ref="yearReconstruction"/>
        <xs:element ref="catalogue"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="owner">
    <xs:complexType mixed="true">
      <xs:attribute name="gnd" type="xs:string"/>
    </xs:complexType>
  </xs:element>
