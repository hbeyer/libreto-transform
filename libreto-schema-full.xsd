        <xs:element name="sections">
          <xs:complexType>
            <xs:choice maxOccurs="unbounded">
              <xs:element ref="section" />
            </xs:choice>            
          </xs:complexType>
        </xs:element>
      </xs:choice>
      <xs:attribute name="id" use="required" type="xs:ID"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="section">
    <xs:complexType mixed="true">
