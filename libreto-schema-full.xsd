        </xs:element>
        <xs:element ref="comment" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Kommentar zum Originalexemplar</xs:documentation>
          </xs:annotation>          
        </xs:element>
      </xs:all>
    </xs:complexType>
