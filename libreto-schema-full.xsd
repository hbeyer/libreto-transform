          <xs:annotation>
            <xs:documentation xml:lang="ger">Besitzende Institution, z. B. "HAB"</xs:documentation>
          </xs:annotation>          
        </xs:element>
        <xs:element minOccurs="1" ref="shelfmark">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Aktuelle Signatur des Originalexemplars</xs:documentation>
          </xs:annotation>         
        </xs:element>
        <xs:element ref="provenanceAttribute" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Merkmal des Originalexemplars, das für die Zuordnung zu der Sammlung maßgeblich ist</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="url" minOccurs="0">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Link zum OPAC-Eintrag des Originalexemplars</xs:documentation>
