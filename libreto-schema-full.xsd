<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
  xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" elementFormDefault="qualified"
  vc:minVersion="1.1">
  <xs:element name="collection">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="metadata"/>
        <xs:choice maxOccurs="unbounded">
          <xs:element ref="item"/>
          <xs:element ref="miscellany"/>
          <xs:element ref="pb"/>
        </xs:choice>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="metadata">
  <xs:annotation>
    <xs:documentation xml:lang="ger">Allgemeine Angaben zur rekonstruierten Sammlung und den verwendeten Quellen</xs:documentation>
  </xs:annotation>
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="heading"/>
        <xs:element ref="owner"/>
        <xs:element ref="description"/>
        <xs:element ref="geoBrowserStorageID"/>
        <xs:element ref="geoBrowserStorageID_bio"/>
        <xs:element ref="yearCollection"/>
        <xs:element ref="yearReconstruction"/>
        <xs:element ref="catalogue"/>
        <xs:element name="person">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Urheber der Rekonstruktion bzw. daran beteiligte Personen (Freitextfeld)</xs:documentation>
          </xs:annotation> 
          <xs:complexType mixed="true">
            <xs:attribute name="role" use="required"></xs:attribute>
            <xs:attribute ref="gender"></xs:attribute>
            <xs:attribute name="gnd"></xs:attribute>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="owner">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Person oder Institution, die Urheber der Sammlung ist</xs:documentation>
        </xs:annotation>
    <xs:complexType mixed="true">
      <xs:attribute name="gnd" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="ger">GND-Nummer der Person oder Institution, die Urheber der Sammlung ist</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute ref="gender"></xs:attribute>      
    </xs:complexType>
  </xs:element>
  <xs:element name="description" type="xs:string">
  <xs:annotation>
      <xs:documentation xml:lang="ger">Abstract zur rekonstruierten Sammlung</xs:documentation>
  </xs:annotation>    
  </xs:element>
  <xs:element name="geoBrowserStorageID" type="xs:string">
    <xs:annotation>
        <xs:documentation xml:lang="ger">ID eines zur Sammlung gehörigen Geodatenblattes im DARIAH Geo-Browser, das sich auf die Druck- bzw. Entstehungsorte bezieht</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="geoBrowserStorageID_bio" type="xs:string">
    <xs:annotation>
        <xs:documentation xml:lang="ger">ID eines zur Sammlung gehörigen Geodatenblattes im DARIAH Geo-Browser, das sich auf die Personen bezieht</xs:documentation>
    </xs:annotation>
  </xs:element>  
  <xs:element name="yearCollection" type="xs:string">
    <xs:annotation>
      <xs:documentation xml:lang="ger">Das Jahr, auf dessen Stand die Sammlung rekonstruiert wird</xs:documentation>
    </xs:annotation> 
  </xs:element>
  <xs:element name="yearReconstruction" type="xs:string">
  <xs:annotation>
    <xs:documentation xml:lang="ger">Jahr, in dem die Rekonstruktion veröffentlicht wurde</xs:documentation>
  </xs:annotation>
  </xs:element>
  <xs:element name="catalogue">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element ref="person"/>
        <xs:element ref="title">
          <xs:annotation>
              <xs:documentation xml:lang="ger">Titel des ausgewerteten Altkatalogs</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="place">
          <xs:annotation>
              <xs:documentation xml:lang="ger">Erscheinungs- oder Entstehungsort des Altkatalogs</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="year" type="xs:string">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Jahr der Publikation oder Entstehung des Altkatalogs</xs:documentation>
          </xs:annotation>           
        </xs:element>
        <xs:element ref="institution">
          <xs:annotation>
              <xs:documentation xml:lang="ger">Institution, die den Altkatalog besitzt</xs:documentation>
          </xs:annotation>          
        </xs:element>
        <xs:element ref="shelfmark">
          <xs:annotation>
              <xs:documentation xml:lang="ger">Signatur des Altkatalogs</xs:documentation>
          </xs:annotation>          
        </xs:element>
        <xs:element ref="base">
          <xs:annotation>
              <xs:documentation xml:lang="ger">URL, mit der man durch Anhängen der Image-Nummer Links auf einzelne Seiten des digitalisierten Altkatalogs bilden kann. Image-Nummern innerhalb der URL werden durch {No} ersetzt.</xs:documentation>
          </xs:annotation>          
        </xs:element>
        <xs:element name="sections">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Die Liste der Rubriken, unter denen der Altkatalog Sammlungsstücke verzeichnet</xs:documentation>
          </xs:annotation> 
          <xs:complexType>
            <xs:choice maxOccurs="unbounded">
              <xs:element ref="section"></xs:element>
            </xs:choice>            
          </xs:complexType>
        </xs:element>
      </xs:choice>
      <xs:attribute name="id" type="xs:ID">
        <xs:annotation>
          <xs:documentation xml:lang="ger">Eindeutige ID für den Katalog</xs:documentation>
        </xs:annotation>                 
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="section">
    <xs:annotation>
      <xs:documentation xml:lang="ger">Rubrik des Altkatalogs, z. B. "Libri theologici"</xs:documentation>
    </xs:annotation> 
    <xs:complexType mixed="true">
      <xs:attribute name="id" use="required" type="xs:ID">
        <xs:annotation>
          <xs:documentation xml:lang="ger">Eindeutige ID für die section</xs:documentation>
        </xs:annotation>         
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="base" type="xs:string"/>
  <xs:element name="heading" type="xs:string">
  <xs:annotation>
    <xs:documentation xml:lang="ger">Titel der rekonstruierten Sammlung</xs:documentation>
  </xs:annotation>    
  </xs:element>
  <xs:element name="title" type="xs:string"/>
  <xs:element name="miscellany">
    <xs:annotation>
      <xs:documentation xml:lang="ger">Sammelband, der die enthaltenen items buchbinderisch zusammenfasst</xs:documentation>
    </xs:annotation> 
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element maxOccurs="unbounded" ref="item"/>
        <xs:element maxOccurs="unbounded" ref="pb"/>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="pb"> <!--Muss auch innerhalb von miscellany stehen können!--> 
    <xs:annotation>
      <xs:documentation xml:lang="ger">Seitenumbruch im Altkatalog</xs:documentation>
    </xs:annotation> 
    <xs:complexType>
      <xs:attribute name="cat" type="xs:IDREF">
        <xs:annotation>
          <xs:documentation xml:lang="ger">ID des Katalogs, auf den sich der Seitenumbruch bezieht</xs:documentation>
        </xs:annotation> 
      </xs:attribute>
      <xs:attribute name="image" use="required" type="xs:integer">
        <xs:annotation>
          <xs:documentation xml:lang="ger">Nummer des Images im Digitalisat</xs:documentation>
        </xs:annotation> 
      </xs:attribute>
      <xs:attribute name="no" use="required" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="ger">Seitenzahl, die in der Regel am Element pb wechselt</xs:documentation>
        </xs:annotation> 
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="year" type="xs:string">
    <xs:annotation>
      <xs:documentation xml:lang="ger">Erscheinungs- oder Entstehungsjahr</xs:documentation>
    </xs:annotation> 
  </xs:element>
  <xs:element name="institution" type="xs:string">
    <xs:annotation>
      <xs:documentation xml:lang="ger">Besitzende Institution</xs:documentation>
    </xs:annotation> 
  </xs:element>
  <xs:element name="shelfmark">
    <xs:annotation>
      <xs:documentation xml:lang="ger">Signatur des Originalexemplars</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:attribute name="epn"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="item">
    <xs:annotation>
      <xs:documentation xml:lang="ger">Ein Sammlungsstück eines beliebigen Medientyps, das unterschiedlich genau erschlossen sein kann</xs:documentation>
    </xs:annotation> 
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element minOccurs="0" ref="id">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Eine eindeutige ID für das Sammlungsstück (wird automatisch ausgefüllt)</xs:documentation>
          </xs:annotation> 
        </xs:element>
        <xs:element minOccurs="0" ref="volumes">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Anzahl der Bände, die das Sammlungsstück umfasste</xs:documentation>
          </xs:annotation> 
        </xs:element>
        <xs:element ref="titleCat">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Titel des Sammlungsstücks im Altkatalog</xs:documentation>
          </xs:annotation> 
        </xs:element>
        <xs:element name="histShelfmark" type="xs:string">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Signatur, die das Sammlunsstück in der historischen Sammlung hatte</xs:documentation>
          </xs:annotation> 
        </xs:element>
        <xs:element minOccurs="0" ref="titleBib"/>
        <xs:element minOccurs="0" ref="titleNormalized"/>
        <xs:element maxOccurs="unbounded" ref="person">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Eine Person, die als VerfasserIn oder in anderer Funktion mit dem Sammlungsstück in Verbindung steht</xs:documentation>
          </xs:annotation> 
        </xs:element>
        <xs:element ref="place">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Ein Erscheinungs- oder Entstehungsort des Sammlungsstückes</xs:documentation>
          </xs:annotation> 
        </xs:element>
        <xs:element ref="publisher">
          <xs:annotation>
            <xs:documentation xml:lang="ger">DruckerIn oder VerlegerIn des Sammlungsstückes, wenn es sich um einen Druck handelt</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="year">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Erscheinungs- oder Entstehungsjahr des Sammlungsstücks</xs:documentation>
          </xs:annotation> 
        </xs:element>
        <xs:element ref="format">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Das bibliographische Format des Sammlungsstückes (bei Drucken oder Handschriften)</xs:documentation>
          </xs:annotation> 
        </xs:element>
        <xs:element maxOccurs="unbounded" ref="subject">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Sachbegriff nach AAD-Liste</xs:documentation>
          </xs:annotation>          
        </xs:element>
        <xs:element minOccurs="0" ref="genre">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Gattungsbegriff nach AAD-Liste</xs:documentation>
          </xs:annotation> 
        </xs:element>
        <xs:element ref="subjectFree"/>
        <xs:element name="language">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Sprache von Text, den das Sammlungsstück enthält</xs:documentation>
          </xs:annotation> 
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <!-- Diese Liste kann durch weitere Sprachbezeichnungen nach ISO 639-2 erweitert werden -->
              <xs:enumeration value="ger"/>
              <xs:enumeration value="lat"/>
              <xs:enumeration value="fre"/>
              <xs:enumeration value="ita"/>
              <xs:enumeration value="spa"/>
              <xs:enumeration value="eng"/>
              <xs:enumeration value="dut"/>
              <xs:enumeration value="ang"/>
              <xs:enumeration value="ara"/>
              <xs:enumeration value="arc"/>
              <xs:enumeration value="arg"/>
              <xs:enumeration value="arm"/>
              <xs:enumeration value="ast"/>
              <xs:enumeration value="ave"/>
              <xs:enumeration value="aze"/>
              <xs:enumeration value="baq"/>
              <xs:enumeration value="bel"/>
              <xs:enumeration value="bre"/>
              <xs:enumeration value="bul"/>
              <xs:enumeration value="cat"/>
              <xs:enumeration value="chi"/>
              <xs:enumeration value="chu"/>
              <xs:enumeration value="cop"/>
              <xs:enumeration value="cos"/>
              <xs:enumeration value="csb"/>
              <xs:enumeration value="cze"/>
              <xs:enumeration value="dan"/>
              <xs:enumeration value="dar"/>
              <xs:enumeration value="dsb"/>
              <xs:enumeration value="dum"/>
              <xs:enumeration value="enm"/>
              <xs:enumeration value="epo"/>
              <xs:enumeration value="est"/>
              <xs:enumeration value="fin"/>
              <xs:enumeration value="frm"/>
              <xs:enumeration value="fro"/>
              <xs:enumeration value="fry"/>
              <xs:enumeration value="fur"/>
              <xs:enumeration value="geo"/>
              <xs:enumeration value="gla"/>
              <xs:enumeration value="gle"/>
              <xs:enumeration value="glg"/>
              <xs:enumeration value="gmh"/>
              <xs:enumeration value="goh"/>
              <xs:enumeration value="got"/>
              <xs:enumeration value="grc"/>
              <xs:enumeration value="gre"/>
              <xs:enumeration value="gsw"/>
              <xs:enumeration value="heb"/>
              <xs:enumeration value="hin"/>
              <xs:enumeration value="hit"/>
              <xs:enumeration value="hrv"/>
              <xs:enumeration value="hsb"/>
              <xs:enumeration value="hun"/>
              <xs:enumeration value="ice"/>
              <xs:enumeration value="ind"/>
              <xs:enumeration value="jpn"/>
              <xs:enumeration value="kal"/>
              <xs:enumeration value="kor"/>
              <xs:enumeration value="kur"/>
              <xs:enumeration value="lad"/>
              <xs:enumeration value="lav"/>
              <xs:enumeration value="lit"/>
              <xs:enumeration value="ltz"/>
              <xs:enumeration value="mac"/>
              <xs:enumeration value="mga"/>
              <xs:enumeration value="mis"/>
              <xs:enumeration value="mlt"/>
              <xs:enumeration value="mnc"/>
              <xs:enumeration value="mol"/>
              <xs:enumeration value="mon"/>
              <xs:enumeration value="nap"/>
              <xs:enumeration value="nds"/>
              <xs:enumeration value="non"/>
              <xs:enumeration value="nor"/>
              <xs:enumeration value="oci"/>
              <xs:enumeration value="ota"/>
              <xs:enumeration value="pal"/>
              <xs:enumeration value="peo"/>
              <xs:enumeration value="per"/>
              <xs:enumeration value="phn"/>
              <xs:enumeration value="pol"/>
              <xs:enumeration value="por"/>
              <xs:enumeration value="pro"/>
              <xs:enumeration value="pus"/>
              <xs:enumeration value="qhe"/>
              <xs:enumeration value="roh"/>
              <xs:enumeration value="rom"/>
              <xs:enumeration value="rum"/>
              <xs:enumeration value="rus"/>
              <xs:enumeration value="san"/>
              <xs:enumeration value="sco"/>
              <xs:enumeration value="sga"/>
              <xs:enumeration value="sla"/>
              <xs:enumeration value="slo"/>
              <xs:enumeration value="slv"/>
              <xs:enumeration value="srd"/>
              <xs:enumeration value="srp"/>
              <xs:enumeration value="sux"/>
              <xs:enumeration value="swa"/>
              <xs:enumeration value="swe"/>
              <xs:enumeration value="syr"/>
              <xs:enumeration value="tha"/>
              <xs:enumeration value="tib"/>
              <xs:enumeration value="tuk"/>
              <xs:enumeration value="tur"/>
              <xs:enumeration value="ukr"/>
              <xs:enumeration value="und"/>
              <xs:enumeration value="uzb"/>
              <xs:enumeration value="vie"/>
              <xs:enumeration value="vol"/>
              <xs:enumeration value="wel"/>
              <xs:enumeration value="wen"/>
              <xs:enumeration value="yid"/>
              <xs:enumeration value="mul"/>
            </xs:restriction>
          </xs:simpleType>          
        </xs:element>
        <xs:element minOccurs="0" ref="work">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Ein Werk, das das Sammlungsstück enthält</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" ref="manifestation">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Eine Ausgabe, die das Sammlungsstück enthält</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element minOccurs="0" ref="originalItem">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Das Originalexemplar, sofern es identifiziert werden kann</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="bound">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Bei Büchern Angabe, ob es sich um gebundenes Material handelt. Standard ist gebunden</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="digi" type="xs:string">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Digitalisat eines beliebigen Exemplars der Ausgabe (vgl. originalItem)</xs:documentation>
          </xs:annotation>
        </xs:element>        
        <xs:element ref="copy">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Ein Exemplar der Ausgabe, das nicht notwendigerweise das Originalexemplar ist</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element ref="comment">
          <xs:annotation>
            <xs:documentation xml:lang="ger">Kommentar (Freitextfeld)</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="id" type="xs:NCName"/>
  <xs:element name="volumes" type="xs:integer"/>
  <xs:element name="titleCat">
    <xs:annotation>
      <xs:documentation xml:lang="ger">Transkription des Eintrags für das Sammlungsstück im Altkatalog</xs:documentation>
    </xs:annotation>
    <xs:complexType mixed="true">
      <xs:attribute name="cat" type="xs:IDREF">
        <xs:annotation>
          <xs:documentation xml:lang="ger">ID des Katalogs, auf den sich der Eintrag bezieht</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="no" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="ger">Nummer des Sammlungsstücks in der Nummerierung des Altkatalogs</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="section" type="xs:IDREF">
        <xs:annotation>
          <xs:documentation xml:lang="ger">ID der Rubrik, in der sich der Eintrag befindet</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="titleBib" type="xs:string">
    <xs:annotation>
      <xs:documentation xml:lang="ger">Bibliographischer Titel des Sammlungsstückes, sofern es sich um einen bestimmbaren Druck handelt</xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:element name="titleNormalized" type="xs:string">
    <xs:annotation>
      <xs:documentation xml:lang="ger">Normalisierter bibliographischer Titel (optional)</xs:documentation>
    </xs:annotation>    
  </xs:element>
  <xs:element name="person">
    <xs:complexType mixed="true">
      <xs:attribute name="beacon">
        <xs:annotation>
          <xs:documentation xml:lang="ger">Identifier für Nachweissysteme, die die GND-Nummer der Person verzeichnen, getrennt durch Semikolon (wird automatisch ausgefüllt)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="gnd" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="ger">GND-Nummer der Person</xs:documentation>
        </xs:annotation>        
      </xs:attribute>
      <xs:attribute ref="gender"/>
      <xs:attribute name="role" use="required">
        <xs:annotation>
          <xs:documentation xml:lang="ger">Beziehungskennzeichnung (s. RDA D-A-CH 18.5 und Anhang I). Alternativ kann mit den DC-Eigenschaften creator und contributor gearbeitet werden</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="xs:string">
            <xs:enumeration value="AdressatIn"/>
            <xs:enumeration value="ArrangeurIn"/>
            <xs:enumeration value="AusführendeR"/>
            <xs:enumeration value="BildhauerIn"/>
            <xs:enumeration value="BuchgestalterIn"/>
            <xs:enumeration value="BuchkünstlerIn"/>
            <xs:enumeration value="HerausgeberIn"/>
            <xs:enumeration value="IllustratorIn"/>
            <xs:enumeration value="KartografIn"/>
            <xs:enumeration value="KommentatorIn"/>
            <xs:enumeration value="KünstlerIn"/>
            <xs:enumeration value="LibrettistIn"/>
            <xs:enumeration value="Normerlassende Gebietskörperschaft"/>
            <xs:enumeration value="Praeses"/>
            <xs:enumeration value="RespondentIn"/>
            <xs:enumeration value="Sonstige Person, Familie und Körperschaft"/>
            <xs:enumeration value="SponsorIn"/>
            <xs:enumeration value="StecherIn"/>
            <xs:enumeration value="TextdichterIn"/>
            <xs:enumeration value="ÜbersetzerIn"/>
            <xs:enumeration value="VerfasserIn"/>
            <xs:enumeration value="VerfasserIn von ergänzendem Text"/>
            <xs:enumeration value="WidmendeR"/>
            <xs:enumeration value="WidmungsempfängerIn"/>
            <xs:enumeration value="ZusammenstellendeR"/>
            <xs:enumeration value="creator"/>
            <xs:enumeration value="contributor"/>
            <xs:enumeration value="borrower"/>
            <xs:enumeration value="VorbesitzerIn"/>
            <xs:enumeration value="ÜbersenderIn"/>
          </xs:restriction>
        </xs:simpleType>        
      </xs:attribute>
      <xs:attribute name="dateLending" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="ger">Das Ausleihdatum (nur sinnvoll, wenn role mit borrower belegt ist)</xs:documentation>
        </xs:annotation>        
      </xs:attribute>
      <xs:attribute name="dateReturning" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="ger">Das (optionale) Rückgabedatum (nur sinnvoll, wenn role mit borrower belegt ist)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute name="provenanceAttribute" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="ger">Ein oder mehrere Provenienzmerkmale (nur sinnvoll, wenn role mit VorbesitzerIn belegt ist)</xs:documentation>
        </xs:annotation>
      </xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="place">
    <xs:complexType>
      <xs:simpleContent>
        <xs:extension base="xs:string">
          <xs:attribute name="geoData">
            <xs:annotation>
              <xs:documentation xml:lang="ger">Die Geodaten des Ortes in der Form 47.55839,7.57327</xs:documentation>
            </xs:annotation>            
          </xs:attribute>
          <xs:attribute name="geoNames" type="xs:string">
            <xs:annotation>
              <xs:documentation xml:lang="ger">Identifier des Ortes bei geonames.org (bevorzugt)</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="getty" type="xs:string">
            <xs:annotation>
              <xs:documentation xml:lang="ger">Identifier des Ortes im Getty Thesaurus</xs:documentation>
            </xs:annotation>
          </xs:attribute>
          <xs:attribute name="gnd" type="xs:string">
            <xs:annotation>
              <xs:documentation xml:lang="ger">GND-Nummer des Ortes</xs:documentation>
            </xs:annotation>
          </xs:attribute>
        </xs:extension>
      </xs:simpleContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="publisher">
    <xs:complexType mixed="true">
      <xs:attribute name="gnd" type="xs:string">
        <xs:annotation>
          <xs:documentation xml:lang="ger">GND-Nummer der druckenden oder verlegenden Person</xs:documentation>
        </xs:annotation>
      </xs:attribute>
      <xs:attribute ref="gender"></xs:attribute>
    </xs:complexType>
  </xs:element>
  <xs:element name="format" type="xs:string"/>
  <xs:element name="histSubject" type="xs:string"/>
