cke: deur Martijn Everart B.";s:15:"titleNormalized";N;s:7:"persons";a:2:{i:0;O:6:"person":6:{s:8:"persName";s:13:"Lopes, Duarte";s:3:"gnd";s:9:"119242613";s:6:"gender";N;s:4:"role";s:6:"author";s:6:"beacon";a:0:{}s:11:"dateLending";N;}i:1;O:6:"person":6:{s:8:"persName";s:18:"Pigafetta, Filippo";s:3:"gnd";s:9:"120560194";s:6:"gender";N;s:4:"role";s:6:"author";s:6:"beacon";a:0:{}s:11:"dateLending";N;}}s:6:"places";a:1:{i:0;O:5:"place":5:{s:9:"placeName";s:9:"Amsterdam";s:8:"geoNames";N;s:3:"gnd";N;s:5:"getty";s:7:"7006952";s:7:"geoData";a:2:{s:3:"lat";s:5:"52.35";s:4:"long";s:6:"4.9167";}}}s:10:"publishers";a:1:{i:0;s:16:"Claesz, Cornelis";}s:4:"year";s:4:"1596";s:6:"format";s:3:"4°";s:11:"histSubject";s:17:"Libri Mathematici";s:13:"histShelfmark";N;s:8:"subjects";a:1:{i:0;s:10:"Geographie";}s:6:"genres";a:0:{}s:9:"mediaType";s:5:"Druck";s:9:"languages";a:1:{i:0;s:3:"dut";}s:13:"manifestation";a:2:{s:19:"systemManifestation";s:3:"GBV";s:15:"idManifestation";s:9:"141129263";}s:12:"originalItem";a:6:{s:19:"institutionOriginal";s:0:"";s:17:"shelfmarkOriginal";s:0:"";s:19:"provenanceAttribute";s:0:"";s:19:"digitalCopyOriginal";s:0:"";s:10:"targetOPAC";s:0:"";s:8:"searchID";s:0:"";}s:4:"work";a:3:{s:9:"titleWork";s:0:"";s:10:"systemWork";s:0:"";s:6:"idWork";s:0:"";}s:5:"bound";s:1:"1";s:7:"comment";s:274:"Siehe auch STCN 832912816. Laut GBV-Aufnahme sind Duarte Lopes und Filippo Pigafetta Verfasser, im STCN ist allerdings nur Filippo Pigafetta als Verfasser vermerkt. Impressum in der STCN-Aufnahme umfangreicher (2. Ort Middelburg, 2. Drucker/Verleger: Jeronimus Wullebrechts)";s:11:"digitalCopy";N;s:9:"copiesHAB";a:0:{}}i:1045;O:4:"item":29:{s:2:"id";s:11:"bahnsen1045";s:7:"pageCat";s:2:"25";s:8:"imageCat";s:2:"27";s:9:"numberCat";s:3:"113";s:12:"itemInVolume";s:1:"0";s:7:"volumes";s:1:"1";s:11:"volumesMisc";N;s:10:"volumeNote";a:2:{s:4:"misc";s:0:"";s:12:"positionMisc";s:0:"";}s:8:"titleCat";s:52:"Spiegel der Spaensche Tyrannye in West-Indien. 1596.";s:8:"titleBib";s:407: