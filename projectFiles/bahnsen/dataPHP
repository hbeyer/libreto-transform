uum <dt.>";s:7:"persons";a:2:{i:0;O:6:"person":6:{s:8:"persName";s:17:"Du Chesne, Joseph";s:3:"gnd";s:9:"124523072";s:6:"gender";N;s:4:"role";s:6:"author";s:6:"beacon";a:5:{i:0;s:3:"wkp";i:1;s:2:"db";i:2;s:3:"hls";i:3;s:4:"kall";i:4;s:3:"dpi";}s:11:"dateLending";N;}i:1;O:6:"person":6:{s:8:"persName";s:15:"Kessler, Thomas";s:3:"gnd";s:9:"124556922";s:6:"gender";N;s:4:"role";s:11:"contributor";s:6:"beacon";a:0:{}s:11:"dateLending";N;}}s:6:"places";a:1:{i:0;O:5:"place":5:{s:9:"placeName";s:10:"Straßburg";s:8:"geoNames";N;s:3:"gnd";N;s:5:"getty";s:7:"7012346";s:7:"geoData";a:2:{s:3:"lat";s:7:"48.5833";s:4:"long";s:4:"7.75";}}}s:10:"publishers";a:1:{i:0;s:17:"Zetzner, Eberhard";}s:4:"year";s:4:"1634";s:6:"format";s:3:"4°";s:11:"histSubject";s:13:"Libri Chymici";s:13:"histShelfmark";N;s:8:"subjects";a:1:{i:0;s:8:"Alchemie";}s:6:"genres";a:1:{i:0;s:10:"Arzneibuch";}s:9:"mediaType";s:5:"Druck";s:9:"languages";a:1:{i:0;s:3:"ger";}s:13:"manifestation";a:2:{s:19:"systemManifestation";s:4:"VD17";s:15:"idManifestation";s:10:"23:290983Q";}s:12:"originalItem";a:6:{s:19:"institutionOriginal";s:0:"";s:17:"shelfmarkOriginal";s:0:"";s:19:"provenanceAttribute";s:0:"";s:19:"digitalCopyOriginal";s:0:"";s:10:"targetOPAC";s:0:"";s:8:"searchID";s:0:"";}s:4:"work";a:3:{s:9:"titleWork";s:0:"";s:10:"systemWork";s:0:"";s:6:"idWork";s:0:"";}s:5:"bound";s:1:"1";s:7:"comment";s:138:"Brüning, V. F. Bibliographie der alchemistischen Literatur 1590. Unter dem Namen Josephus Quercetanus (d.i. Joseph Du Chesne) erschienen.";s:11:"digitalCopy";s:49:"http://diglib.hab.de/drucke/38-8-med-2s/start.htm";s:9:"copiesHAB";a:0:{}}i:605;O:4:"item":29:{s:2:"id";s:10:"bahnsen605";s:7:"pageCat";s:2:"15";s:8:"imageCat";s:2:"73";s:9:"numberCat";s:2:"19";s:12:"itemInVolume";s:1:"0";s:7:"volumes";s:1:"1";s:11:"volumesMisc";N;s:10:"volumeNote";a:2:{s:4:"misc";s:0:"";s:12:"positionMisc";s:0:"";}s:8:"titleCat";s:31:"Plinius Secundus. Nederduytsch.";s:8:"titleBib";N;s:15:"titleNormalized";N;s:7:"persons";a:0:{}s:6:"places"