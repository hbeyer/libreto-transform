am sylvam: Post Salvatoris nativitatem 1617.]";s:15:"titleNormalized";s:79:"De Naturae Secretis Quibusdam Ad Vulcaniam Artis Chimiae ante omnia necessariis";s:7:"persons";a:0:{}s:6:"places";a:1:{i:0;O:5:"place":5:{s:9:"placeName";s:6:"Erfurt";s:8:"geoNames";N;s:3:"gnd";N;s:5:"getty";s:7:"7024031";s:7:"geoData";a:2:{s:3:"lat";s:7:"50.9833";s:4:"long";s:7:"11.0333";}}}s:10:"publishers";a:1:{i:0;s:4:"S.n.";}s:4:"year";s:4:"1618";s:6:"format";s:3:"8°";s:11:"histSubject";s:13:"Libri Chymici";s:13:"histShelfmark";N;s:8:"subjects";a:1:{i:0;s:8:"Alchemie";}s:6:"genres";a:1:{i:0;s:20:"Gesellschaftsschrift";}s:9:"mediaType";s:5:"Druck";s:9:"languages";a:1:{i:0;s:3:"ger";}s:13:"manifestation";a:2:{s:19:"systemManifestation";s:4:"VD17";s:15:"idManifestation";s:10:"14:050839E";}s:12:"originalItem";a:6:{s:19:"institutionOriginal";s:0:"";s:17:"shelfmarkOriginal";s:0:"";s:19:"provenanceAttribute";s:0:"";s:19:"digitalCopyOriginal";s:0:"";s:10:"targetOPAC";s:0:"";s:8:"searchID";s:0:"";}s:4:"work";a:3:{s:9:"titleWork";s:0:"";s:10:"systemWork";s:0:"";s:6:"idWork";s:0:"";}s:5:"bound";s:1:"1";s:7:"comment";s:39:"Bibliogr. Nachweis: Wolfstieg II, 42332";s:11:"digitalCopy";s:43:"http://digital.slub-dresden.de/ppn27063049X";s:9:"copiesHAB";a:0:{}}i:679;O:4:"item":29:{s:2:"id";s:10:"bahnsen679";s:7:"pageCat";s:2:"17";s:8:"imageCat";s:2:"19";s:9:"numberCat";s:1:"9";s:12:"itemInVolume";s:1:"3";s:7:"volumes";s:1:"1";s:11:"volumesMisc";N;s:10:"volumeNote";a:2:{s:4:"misc";s:24:"br:bahnsen/miscellany_46";s:12:"positionMisc";s:1:"3";}s:8:"titleCat";s:20:"Sylloge de Fr. R. C.";s:8:"titleBib";N;s:15:"titleNormalized";N;s:7:"persons";a:0:{}s:6:"places";a:0:{}s:10:"publishers";a:0:{}s:4:"year";N;s:6:"format";s:3:"8°";s:11:"histSubject";s:13:"Libri Chymici";s:13:"histShelfmark";N;s:8:"subjects";a:0:{}s:6:"genres";a:0:{}s:9:"mediaType";s:5:"Druck";s:9:"languages";a:0:{}s:13:"manifestation";a:2:{s:19:"systemManifestation";s:0:"";s:15:"idManifestation";s:0:"";}s:12:"originalItem";a:6:{s:19:"institutionOrig