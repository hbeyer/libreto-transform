on, Johannes van";i:1;s:17:"Loon, Gielles van";}s:4:"year";s:4:"1651";s:6:"format";s:3:"4°";s:11:"histSubject";s:17:"Libri Mathematici";s:13:"histShelfmark";N;s:8:"subjects";a:1:{i:0;s:10:"Mathematik";}s:6:"genres";a:0:{}s:9:"mediaType";s:5:"Druck";s:9:"languages";a:1:{i:0;s:3:"dut";}s:13:"manifestation";a:2:{s:19:"systemManifestation";s:4:"STCN";s:15:"idManifestation";s:9:"121700178";}s:12:"originalItem";a:6:{s:19:"institutionOriginal";s:0:"";s:17:"shelfmarkOriginal";s:0:"";s:19:"provenanceAttribute";s:0:"";s:19:"digitalCopyOriginal";s:0:"";s:10:"targetOPAC";s:0:"";s:8:"searchID";s:0:"";}s:4:"work";a:3:{s:9:"titleWork";s:0:"";s:10:"systemWork";s:0:"";s:6:"idWork";s:0:"";}s:5:"bound";s:1:"1";s:7:"comment";N;s:11:"digitalCopy";N;s:9:"copiesHAB";a:0:{}}i:1052;O:4:"item":29:{s:2:"id";s:11:"bahnsen1052";s:7:"pageCat";s:2:"26";s:8:"imageCat";s:2:"28";s:9:"numberCat";s:3:"120";s:12:"itemInVolume";s:1:"0";s:7:"volumes";s:1:"1";s:11:"volumesMisc";N;s:10:"volumeNote";a:2:{s:4:"misc";s:0:"";s:12:"positionMisc";s:0:"";}s:8:"titleCat";s:46:"Hood. 't Gebruyck van de Zeekaert. Amst. 1602.";s:8:"titleBib";N;s:15:"titleNormalized";N;s:7:"persons";a:0:{}s:6:"places";a:0:{}s:10:"publishers";a:0:{}s:4:"year";N;s:6:"format";s:3:"4°";s:11:"histSubject";s:17:"Libri Mathematici";s:13:"histShelfmark";N;s:8:"subjects";a:0:{}s:6:"genres";a:0:{}s:9:"mediaType";s:5:"Druck";s:9:"languages";a:0:{}s:13:"manifestation";a:2:{s:19:"systemManifestation";s:0:"";s:15:"idManifestation";s:0:"";}s:12:"originalItem";a:6:{s:19:"institutionOriginal";s:0:"";s:17:"shelfmarkOriginal";s:0:"";s:19:"provenanceAttribute";s:0:"";s:19:"digitalCopyOriginal";s:0:"";s:10:"targetOPAC";s:0:"";s:8:"searchID";s:0:"";}s:4:"work";a:3:{s:9:"titleWork";s:0:"";s:10:"systemWork";s:0:"";s:6:"idWork";s:0:"";}s:5:"bound";s:1:"1";s:7:"comment";N;s:11:"digitalCopy";N;s:9:"copiesHAB";a:0:{}}i:1053;O:4:"item":29:{s:2:"id";s:11:"bahnsen1053";s:7:"pageCat";s:2:"26";s:8:"imageCat";s:2:"28";s:9:"numberCat";s:3:"121";s:12:"itemInVolume";s:1: