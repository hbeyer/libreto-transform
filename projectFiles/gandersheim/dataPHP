ation";a:2:{s:19:"systemManifestation";s:4:"VD17";s:15:"idManifestation";s:9:"1:073701P";}s:12:"originalItem";a:6:{s:19:"institutionOriginal";s:0:"";s:17:"shelfmarkOriginal";s:0:"";s:19:"provenanceAttribute";s:0:"";s:19:"digitalCopyOriginal";s:0:"";s:10:"targetOPAC";s:0:"";s:8:"searchID";s:0:"";}s:4:"work";a:3:{s:9:"titleWork";s:0:"";s:10:"systemWork";s:0:"";s:6:"idWork";s:0:"";}s:5:"bound";s:1:"1";s:7:"comment";N;s:11:"digitalCopy";N;s:9:"copiesHAB";a:0:{}}i:585;O:4:"item":29:{s:2:"id";s:14:"gandersheim585";s:7:"pageCat";s:2:"26";s:8:"imageCat";N;s:9:"numberCat";s:1:"3";s:12:"itemInVolume";s:1:"0";s:7:"volumes";s:1:"1";s:11:"volumesMisc";N;s:10:"volumeNote";a:2:{s:4:"misc";s:0:"";s:12:"positionMisc";s:0:"";}s:8:"titleCat";s:29:"Effigies regum Galliae. 1576.";s:8:"titleBib";s:72:"Effigies Regvm Francorvm Omnivm, A Pharamvndo, ad Henricvm vsqve Tertivm";s:15:"titleNormalized";N;s:7:"persons";a:3:{i:0;O:6:"person":6:{s:8:"persName";s:16:"Bernard, Georges";s:3:"gnd";s:9:"128843853";s:6:"gender";s:1:"m";s:4:"role";s:6:"author";s:6:"beacon";a:0:{}s:11:"dateLending";N;}i:1;O:6:"person":6:{s:8:"persName";s:13:"Solis, Virgil";s:3:"gnd";s:9:"118615300";s:6:"gender";s:1:"m";s:4:"role";s:11:"contributor";s:6:"beacon";a:7:{i:0;s:3:"wkp";i:1;s:2:"db";i:2;s:4:"pbbl";i:3;s:8:"sandrart";i:4;s:4:"kall";i:5;s:3:"dpi";i:6;s:3:"vkk";}s:11:"dateLending";N;}i:2;O:6:"person":6:{s:8:"persName";s:10:"Amman, Jos";s:3:"gnd";s:9:"118502573";s:6:"gender";s:1:"m";s:4:"role";s:11:"contributor";s:6:"beacon";a:11:{i:0;s:3:"wkp";i:1;s:3:"ddb";i:2;s:2:"db";i:3;s:3:"hls";i:4;s:4:"pbbl";i:5;s:4:"hbio";i:6;s:4:"gspd";i:7;s:8:"sandrart";i:8;s:4:"kall";i:9;s:3:"dpi";i:10;s:3:"vkk";}s:11:"dateLending";N;}}s:6:"places";a:1:{i:0;O:5:"place":5:{s:9:"placeName";s:9:"Nürnberg";s:8:"geoNames";N;s:3:"gnd";N;s:5:"getty";s:7:"7004334";s:7:"geoData";a:2:{s:3:"lat";s:9:"49.447778";s:4:"long";s:9:"11.068333";}}}s:10:"publishers";a:1:{i:0;s:18:"Gerlach, Katharina";}s:4:"year";s:4:"1576";s:6:"format";s:3:"4°";s:11:"hist