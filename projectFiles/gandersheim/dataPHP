lichen Häusern";s:15:"titleNormalized";N;s:7:"persons";a:2:{i:0;O:6:"person":6:{s:8:"persName";s:20:"Fugger, Johann Jakob";s:3:"gnd";s:9:"118703501";s:6:"gender";s:1:"m";s:4:"role";s:6:"author";s:6:"beacon";a:10:{i:0;s:3:"wkp";i:1;s:3:"ddb";i:2;s:2:"db";i:3;s:3:"hls";i:4;s:4:"phoh";i:5;s:8:"sandrart";i:6;s:4:"kall";i:7;s:3:"zdn";i:8;s:3:"dpi";i:9;s:3:"vkk";}s:11:"dateLending";N;}i:1;O:6:"person":6:{s:8:"persName";s:20:"Birken, Sigmund, von";s:3:"gnd";s:9:"118511270";s:6:"gender";s:1:"m";s:4:"role";s:6:"author";s:6:"beacon";a:12:{i:0;s:3:"wkp";i:1;s:2:"db";i:2;s:4:"gesa";i:3;s:13:"fruchtbringer";i:4;s:8:"sandrart";i:5;s:4:"kall";i:6;s:3:"zdn";i:7;s:3:"dta";i:8;s:3:"dpi";i:9;s:3:"vkk";i:10;s:5:"imslp";i:11;s:4:"cmvw";}s:11:"dateLending";N;}}s:6:"places";a:1:{i:0;O:5:"place":5:{s:9:"placeName";s:9:"Nürnberg";s:8:"geoNames";N;s:3:"gnd";N;s:5:"getty";s:7:"7004334";s:7:"geoData";a:2:{s:3:"lat";s:9:"49.447778";s:4:"long";s:9:"11.068333";}}}s:10:"publishers";a:2:{i:0;s:15:"Endter, Michael";i:1;s:24:"Endter, Johann Friedrich";}s:4:"year";s:4:"1668";s:6:"format";s:3:"2°";s:11:"histSubject";s:16:"Schrank 4. Folio";s:13:"histShelfmark";N;s:8:"subjects";a:2:{i:0;s:10:"Geschichte";i:1;s:10:"Genealogie";}s:6:"genres";a:0:{}s:9:"mediaType";s:4:"book";s:9:"languages";a:1:{i:0;s:3:"ger";}s:13:"manifestation";a:2:{s:19:"systemManifestation";s:4:"VD17";s:15:"idManifestation";s:10:"23:231732Y";}s:12:"originalItem";a:6:{s:19:"institutionOriginal";s:23:"Landesbibliothek Coburg";s:17:"shelfmarkOriginal";s:7:"K I 2/6";s:19:"provenanceAttribute";s:12:"Exlibris EEA";s:19:"digitalCopyOriginal";s:0:"";s:10:"targetOPAC";s:0:"";s:8:"searchID";s:0:"";}s:4:"work";a:3:{s:9:"titleWork";s:0:"";s:10:"systemWork";s:0:"";s:6:"idWork";s:0:"";}s:5:"bound";s:1:"1";s:7:"comment";N;s:11:"digitalCopy";s:39:"http://data.onb.ac.at/ABO/%2BZ10350610X";s:9:"copiesHAB";a:0:{}}i:565;O:4:"item":29:{s:2:"id";s:14:"gandersheim565";s:7:"pageCat";s:2:"26";s:8:"imageCat";N;s:9:"numberCat";s:2:"16";s:12:"itemInVolume";s:1:"