lichen Bildereyen, sorgfältig abgesehen";s:15:"titleNormalized";N;s:7:"persons";a:2:{i:0;O:6:"person":6:{s:8:"persName";s:21:"Sandrart, Joachim von";s:3:"gnd";s:9:"118794396";s:6:"gender";s:1:"m";s:4:"role";s:6:"author";s:6:"beacon";a:10:{i:0;s:3:"wkp";i:1;s:2:"db";i:2;s:4:"pbbl";i:3;s:4:"gspd";i:4;s:13:"fruchtbringer";i:5;s:8:"sandrart";i:6;s:4:"kall";i:7;s:3:"dta";i:8;s:3:"dpi";i:9;s:3:"vkk";}s:11:"dateLending";N;}i:1;O:6:"person":6:{s:8:"persName";s:26:"Sandrart, Johann Jacob von";s:3:"gnd";s:9:"115749438";s:6:"gender";s:1:"m";s:4:"role";s:11:"contributor";s:6:"beacon";a:5:{i:0;s:2:"db";i:1;s:4:"pbbl";i:2;s:4:"gspd";i:3;s:8:"sandrart";i:4;s:3:"dpi";}s:11:"dateLending";N;}}s:6:"places";a:1:{i:0;O:5:"place":5:{s:9:"placeName";s:9:"Nürnberg";s:8:"geoNames";N;s:3:"gnd";N;s:5:"getty";s:7:"7004334";s:7:"geoData";a:2:{s:3:"lat";s:9:"49.447778";s:4:"long";s:9:"11.068333";}}}s:10:"publishers";a:2:{i:0;s:17:"Riegel, Christoph";i:1;s:27:"Froberg, Christian Siegmund";}s:4:"year";s:4:"1680";s:6:"format";s:3:"2°";s:11:"histSubject";s:16:"Schrank 9. Folio";s:13:"histShelfmark";N;s:8:"subjects";a:3:{i:0;s:14:"Altertumskunde";i:1;s:14:"antike Götter";i:2;s:12:"Ikonographie";}s:6:"genres";a:0:{}s:9:"mediaType";s:4:"book";s:9:"languages";a:1:{i:0;s:3:"ger";}s:13:"manifestation";a:2:{s:19:"systemManifestation";s:4:"VD17";s:15:"idManifestation";s:10:"14:019650Q";}s:12:"originalItem";a:6:{s:19:"institutionOriginal";s:23:"Landesbibliothek Coburg";s:17:"shelfmarkOriginal";s:8:"A I 1/20";s:19:"provenanceAttribute";s:34:"Exlibris und hs. Besitzvermerk EEA";s:19:"digitalCopyOriginal";s:0:"";s:10:"targetOPAC";s:0:"";s:8:"searchID";s:0:"";}s:4:"work";a:3:{s:9:"titleWork";s:0:"";s:10:"systemWork";s:0:"";s:6:"idWork";s:0:"";}s:5:"bound";s:1:"1";s:7:"comment";N;s:11:"digitalCopy";s:46:"http://diglib.hab.de/drucke/ed000026/start.htm";s:9:"copiesHAB";a:0:{}}i:1129;O:4:"item":29:{s:2:"id";s:15:"gandersheim1129";s:7:"pageCat";s:2:"26";s:8:"imageCat";N;s:9:"numberCat";s:1:"4";s:12:"itemInVolume";s:1