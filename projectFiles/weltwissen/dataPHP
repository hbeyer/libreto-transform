35";s:4:"long";s:6:"4.9167";}}}s:10:"publishers";a:1:{i:0;s:14:"Mortier, David";}s:13:"publishersObj";a:1:{i:0;O:9:"publisher":2:{s:4:"name";s:14:"Mortier, David";s:3:"gnd";s:10:"1037565371";}}s:4:"year";s:4:"1718";s:6:"format";s:5:"Folio";s:11:"histSubject";N;s:13:"histShelfmark";s:0:"";s:8:"subjects";a:2:{i:0;s:19:"Literaturgeschichte";i:1;s:10:"Geschichte";}s:6:"genres";a:0:{}s:9:"mediaType";N;s:9:"languages";a:1:{i:0;s:3:"fre";}s:13:"manifestation";a:3:{s:19:"systemManifestation";s:0:"";s:15:"idManifestation";s:0:"";s:20:"commentManifestation";s:0:"";}s:12:"originalItem";a:8:{s:19:"institutionOriginal";s:3:"HAB";s:17:"shelfmarkOriginal";s:11:"M: Lm 2° 2";s:19:"provenanceAttribute";s:77:"“ELISABETH SOPHIA MARIA. Verwittwete HERZOGINN ZU BR. U. LUN:” (Band 1-2)";s:19:"digitalCopyOriginal";s:0:"";s:10:"targetOPAC";s:0:"";s:8:"searchID";s:0:"";s:8:"OPACLink";s:67:"http://opac.lbs-braunschweig.gbv.de/DB=2/XMLPRS=N/PPN?PPN=151421129";s:15:"commentOriginal";s:25:"Altsignatur: P. I. e. 1-2";}s:4:"work";a:3:{s:9:"titleWork";s:0:"";s:10:"systemWork";s:0:"";s:6:"idWork";s:0:"";}s:5:"bound";s:1:"1";s:7:"comment";s:8:"Teil 1-2";s:11:"digitalCopy";N;s:9:"copiesHAB";a:0:{}}i:28;O:4:"item":31:{s:2:"id";s:12:"weltwissen28";s:7:"pageCat";N;s:8:"imageCat";N;s:9:"numberCat";N;s:12:"itemInVolume";i:1;s:7:"volumes";s:1:"2";s:11:"volumesMisc";N;s:10:"volumeNote";a:2:{s:4:"misc";s:27:"br:weltwissen/miscellany_28";s:12:"positionMisc";i:1;}s:8:"titleCat";s:27:"Annales des Provinces unies";s:10:"catEntries";a:1:{i:0;O:15:"catalogue_entry":6:{s:5:"idCat";N;s:8:"titleCat";s:27:"Annales des Provinces unies";s:9:"numberCat";N;s:7:"pageCat";N;s:8:"imageCat";N;s:11:"histSubject";N;}}s:8:"titleBib";s:27:"Annales Des Provinces-Unies";s:15:"titleNormalized";s:27:"Annales des Provinces-Unies";s:7:"persons";a:1:{i:0;O:6:"person":6:{s:8:"persName";s:27:"Basnage de Beauval, Jacques";s:3:"gnd";s:9:"120538253";s:6:"gender";s:1:"m";s:4:"role";s:11:"VerfasserIn";s:6:"beacon";a:6:{i:0;s:3:"ddb";i:1;s:3:"hl