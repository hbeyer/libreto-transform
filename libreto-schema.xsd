<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="collection">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="1" minOccurs="0" ref="metadata"/>
                <xs:element maxOccurs="unbounded" minOccurs="1" ref="item"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="metadata">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Allgemeine Angaben zur rekonstruierten Sammlung und den verwendeten Quellen</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:all>
                <xs:element maxOccurs="1" ref="heading"/>
                <xs:element minOccurs="0" maxOccurs="1" ref="owner"/>
                <xs:element minOccurs="0" maxOccurs="1" ref="ownerGND"/>
                <xs:element maxOccurs="1" ref="fileName"/>
                <xs:element minOccurs="0" maxOccurs="1" ref="title"/>
                <xs:element minOccurs="0" maxOccurs="1" ref="base"/>
                <xs:element minOccurs="0" maxOccurs="1" ref="placeCat"/>
                <xs:element maxOccurs="1" ref="year"/>
                <xs:element minOccurs="1" ref="institution"/>
                <xs:element minOccurs="0" maxOccurs="1" ref="shelfmark"/>
                <xs:element minOccurs="0" maxOccurs="1" ref="description"/>
                <xs:element minOccurs="0" maxOccurs="1" ref="geoBrowserStorageID"/>
                <xs:element minOccurs="0" ref="creatorReconstruction"/>
                <xs:element minOccurs="0" maxOccurs="1" ref="yearReconstruction"/>
            </xs:all>
        </xs:complexType>
    </xs:element>
    <xs:element name="heading" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Titel der rekonstruierten Sammlung</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="owner" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Person oder Institution, die Urheber der Sammlung ist</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="ownerGND" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">GND-Nummer der Person oder Institution, die Urheber der Sammlung ist</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="fileName" type="xs:normalizedString">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Dateiname für den Projektordner sowie zur Bildung von IDs</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="title" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Titel des ausgewerteten Altkatalogs</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="base" type="xs:anyURI">
        <xs:annotation>
            <xs:documentation xml:lang="ger">URL, mit der man durch Anhängen der Image-Nummer Links auf einzelne Seiten des digitalisierten Altkatalogs bilden kann. Image-Nummern innerhalb der URL werden durch {No} ersetzt.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="placeCat" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Erscheinungs- oder Entstehungsort des Altkatalogs</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="institution" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Institution, die den Altkatalog besitzt</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="shelfmark" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Signatur des Altkatalogs</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="description" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Abstract zur rekonstruierten Sammlung</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="geoBrowserStorageID" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">ID eines zur Sammlung gehörigen Geodatenblattes im DARIAH Geo-Browser</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="creatorReconstruction" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Urheber der Rekonstruktion bzw. daran beteiligte Personen (Freitextfeld)</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="yearReconstruction" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Jahr, in dem die Rekonstruktion zuerst veröffentlicht wurde</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="item">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Ein Sammlungsstück in der zu erschließenden historischen Sammlung (auch Titel genannt). Medientyp und bibliographisches Level ergeben sich aus den erhobenen Daten</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:all>
                <xs:element minOccurs="0" ref="titleCat"/>
                <xs:element minOccurs="0" ref="titleBib"/>
                <xs:element minOccurs="0" ref="id"/>
                <xs:element minOccurs="0" ref="pageCat"/>
                <xs:element minOccurs="0" ref="imageCat"/>
                <xs:element minOccurs="0" ref="numberCat"/>
                <xs:element minOccurs="0" ref="itemInVolume"/>
                <xs:element minOccurs="0" ref="volumes"/>
                <xs:element minOccurs="0" ref="persons"/>
                <xs:element minOccurs="0" ref="titleNormalized"/>
                <xs:element minOccurs="0" ref="places"/>
                <xs:element minOccurs="0" ref="publishers"/>
                <xs:element minOccurs="0" ref="year"/>
                <xs:element minOccurs="0" ref="format"/>
                <xs:element minOccurs="0" ref="histSubject"/>
                <xs:element minOccurs="0" ref="histShelfmark"/>
                <xs:element minOccurs="0" ref="subjects"/>
                <xs:element minOccurs="0" ref="genres"/>
                <xs:element minOccurs="0" ref="mediaType"/>
                <xs:element minOccurs="0" ref="languages"/>
                <xs:element minOccurs="0" ref="manifestation"/>
                <xs:element minOccurs="0" ref="originalItem"/>
                <xs:element minOccurs="0" ref="work"/>
                <xs:element minOccurs="0" ref="bound"/>
                <xs:element minOccurs="0" ref="comment"/>
                <xs:element minOccurs="0" ref="copiesHAB"/>
                <xs:element minOccurs="0" ref="digitalCopy"/>
            </xs:all>
        </xs:complexType>
    </xs:element>

    <xs:element name="id" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Die ID wird vom System vergeben</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="pageCat" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Seite(n) im Altkatalog, auf der oder denen Titel erscheint</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="imageCat" type="xs:integer">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Image-Nummer der Seite im Digitalisat des Altkatalogs, sofern vorhanden</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="numberCat" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Nummerierung des Titels im Altkatalog in Vorlageform</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="itemInVolume" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Bei Sammelbänden: Zahl, die angibt, das wievielte Teilstück der Titel ist. Das Feld dient zugleich der Kennzeichnung von Sammelbänden. Sollte der Befund am Originalexemplar von den Angaben des Altkatalogs abweichen, ist die Anordnung des Altkatalogs zu bevorzugen und die Abweichung im Feld comment zu vermerken</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="volumes" type="xs:integer">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Anzahl der Bände eines mehrbändigen Werks (auch für mehrbändige Sammelbände verwendbar)</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="titleCat" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Der Wortlaut des Eintrags im Altkatalog mit allen dazugehörigen Angaben wie Drucker und Ort oder Bemerkungen über den Einband</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="titleBib" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Der bibliographierte Titel aus einem bibliographischen Nachweissystem (z. B. VD17). Das Nachweissystem wird unter manifestation nach einer normierten Liste angegeben</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="titleNormalized" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Orthographisch normalisierte Form des Titels für die Anzeige (optional)</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="persons">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Personenliste für Verfasser(innen) und beteiligte Personen. Drucker und Verleger nur als publisher</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="1" ref="person"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="person">
        <xs:complexType>
            <xs:all>
                <xs:element ref="persName"/>
                <xs:element minOccurs="0" ref="gnd"/>
                <xs:element minOccurs="0" ref="gender"/>
                <xs:element minOccurs="0" ref="dateLending"/>
                <xs:element ref="role"/>
            </xs:all>
        </xs:complexType>
    </xs:element>
    <xs:element name="persName" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Bevorzugter Name einer Person</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="gnd" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">GND-Nummer der Person</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="gender">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Indikator für das Geschlecht (optional)</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="m"/>
                <xs:enumeration value="f"/>
                <xs:enumeration value="d"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="dateLending" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Ausleihdatum, möglichst in der Form "YYYY-mm-dd". Nur erlaubt, wenn role "borrower" ist</xs:documentation>
        </xs:annotation>
    </xs:element>    
    <xs:element name="role">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Beziehungskennzeichnung (s. RDA D-A-CH 18.5 und Anhang I). Alternativ kann mit den DC-Eigenschaften creator und contributor gearbeitet werden</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="AdressatIn"/>
                <xs:enumeration value="ArrangeurIn"/>
                <xs:enumeration value="AusführendeR"/>
                <xs:enumeration value="BildhauerIn"/>
                <xs:enumeration value="BuchgestalterIn"/>
                <xs:enumeration value="BuchkünstlerIn"/>
                <xs:enumeration value="HerausgeberIn"/>
                <xs:enumeration value="IllustratorIn"/>
                <xs:enumeration value="KartografIn"/>
                <xs:enumeration value="KommentatorIn"/>
                <xs:enumeration value="KünstlerIn"/>
                <xs:enumeration value="LibrettistIn"/>
                <xs:enumeration value="Normerlassende Gebietskörperschaft"/>
                <xs:enumeration value="Praeses"/>
                <xs:enumeration value="RespondentIn"/>
                <xs:enumeration value="Sonstige Person, Familie und Körperschaft"/>
                <xs:enumeration value="SponsorIn"/>
                <xs:enumeration value="StecherIn"/>
                <xs:enumeration value="TextdichterIn"/>
                <xs:enumeration value="ÜbersetzerIn"/>
                <xs:enumeration value="VerfasserIn"/>
                <xs:enumeration value="VerfasserIn von ergänzendem Text"/>
                <xs:enumeration value="WidmendeR"/>
                <xs:enumeration value="WidmungsempfängerIn"/>
                <xs:enumeration value="ZusammenstellendeR"/>
                <xs:enumeration value="creator"/>
                <xs:enumeration value="contributor"/>
                <xs:enumeration value="borrower"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="places">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Liste mit Erscheinungs- oder Entstehungsorten des Titels. Erschlossene Angaben können in eckige Klammern gesetzt werden</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" ref="place"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="publishers">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Liste mit einem oder mehreren Druckern oder Verlegern</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="1" ref="publisher"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="publisher" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Der oder die Drucker oder Verleger als Freitextfeld. Erschlossene Angaben können in eckige Klammern gesetzt werden</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="format" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Das bibliographische Format. Abweichende Angaben aus dem Altkatalog stehen unter titleCat oder histSubject</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="histSubject" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Die Rubrik, unter der der Titel im Altkatalog geführt wird, z. B. &quot;Libri theologici in 2°&quot;</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="histShelfmark" type="xs:string">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Eine im Altkatalog angegebene oder sonstwie recherchierte historische Signatur</xs:documentation>
        </xs:annotation>
    </xs:element>
    <xs:element name="subjects">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Ein oder mehrere Sachbegriffe (Gattungsbegriffe s. genres) aus der Liste der AAD-Gattungsbegriffe, ergänzt um dort nicht vorhandene Fächer wie Theologie. Die Liste ist im Schema hinterlegt</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="1" ref="subject"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="subject">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <!-- Diese Liste kann erweitert werden, wenn es inhaltlich sinnvoll ist -->
                <xs:enumeration value="Akustik"/>
                <xs:enumeration value="Alchemie"/>
                <xs:enumeration value="Altertumskunde"/>
                <xs:enumeration value="Anatomie"/>
                <xs:enumeration value="Anekdote"/>
                <xs:enumeration value="Archäologie"/>
                <xs:enumeration value="Architektur"/>
                <xs:enumeration value="Astrologie"/>
                <xs:enumeration value="Astronomie"/>
                <xs:enumeration value="Bergbau"/>
                <xs:enumeration value="Botanik"/>
                <xs:enumeration value="Chemie"/>
                <xs:enumeration value="Chiromantie"/>
                <xs:enumeration value="Chirurgie"/>
                <xs:enumeration value="Festungsbau"/>
                <xs:enumeration value="Gartenbau"/>
                <xs:enumeration value="Geldwesen"/>
                <xs:enumeration value="Genealogie"/>
                <xs:enumeration value="Geographie"/>
                <xs:enumeration value="Geologie"/>
                <xs:enumeration value="Geometrie"/>
                <xs:enumeration value="Gynäkologie"/>
                <xs:enumeration value="Kirchengeschichte"/> <!--Hinzugefügt von H. Beyer-->
                <xs:enumeration value="Kriegskunde"/>
                <xs:enumeration value="Kryptologie"/>
                <xs:enumeration value="Landwirtschaft"/>
                <xs:enumeration value="Magnetismus"/>
                <xs:enumeration value="Mathematik"/>
                <xs:enumeration value="Mechanik"/>
                <xs:enumeration value="Medizin"/>
                <xs:enumeration value="Mineralogie"/>
                <xs:enumeration value="Musik"/>
                <xs:enumeration value="Optik"/>
                <xs:enumeration value="Orientalistik"/>
                <xs:enumeration value="Pädagogik"/>
                <xs:enumeration value="Philosophie"/> <!--Hinzugefügt von H. Beyer-->
                <xs:enumeration value="Physik"/>
                <xs:enumeration value="Physiognomie"/>
                <xs:enumeration value="Poetik"/>
                <xs:enumeration value="Rechtswissenschaft"/> <!--Hinzugefügt von H. Beyer-->                
                <xs:enumeration value="Rhetorik"/>
                <xs:enumeration value="Statistik"/>
                <xs:enumeration value="Theologie"/>
                <xs:enumeration value="Topographie"/>
                <xs:enumeration value="Zoologie"/>
            </xs:restriction>
        </xs:simpleType>
    </xs:element>
    <xs:element name="genres">
        <xs:annotation>
            <xs:documentation xml:lang="ger">Ein oder mehrere Gattungsbegriffe (Sachbegriffe s. subjects) aus der Liste der AAD-Gattungsbegriffe. Die Liste ist im Schema hinterlegt</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="1" ref="genre"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:element name="genre">
        <xs:simpleType>
            <xs:restriction base="xs:string">
                <xs:enumeration value="Adressbuch"/>
                <xs:enumeration value="Agende"/>
                <xs:enumeration value="Akademieschrift"/>
                <xs:enumeration value="Almanach"/>
                <xs:enumeration value="Amtsdruckschrift"/>
                <xs:enumeration value="Anleitung"/>
                <xs:enumeration value="Anstandsliteratur"/>
