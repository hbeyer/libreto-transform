                <xs:enumeration value="Antiquariatskatalog"/>
                <xs:enumeration value="Anzeige"/>
                <xs:enumeration value="Aphorismus"/>
                <xs:enumeration value="Ars moriendi"/>
                <xs:enumeration value="Arzneibuch"/>
                <xs:enumeration value="Atlas"/>
                <xs:enumeration value="Auktionskatalog"/>
                <xs:enumeration value="Autobiographie"/>
                <xs:enumeration value="Ballade"/>
                <xs:enumeration value="Beichtspiegel"/>
                <xs:enumeration value="Bericht"/>
                <xs:enumeration value="Bibel"/>
                <xs:enumeration value="Bibliographie"/>
                <xs:enumeration value="Bibliothekskatalog"/>
                <xs:enumeration value="Biographie"/>
                <xs:enumeration value="Brevier"/>
                <xs:enumeration value="Brief"/>
                <xs:enumeration value="Briefsammlung"/>
                <xs:enumeration value="Briefsteller"/>
                <xs:enumeration value="Buchbinderanweisung"/>
                <xs:enumeration value="Buchhandelskatalog"/>
                <xs:enumeration value="Bücheranzeige"/>
                <xs:enumeration value="Chronik"/>
                <xs:enumeration value="Dissertation"/>
                <xs:enumeration value="Dissertation:theol."/>
                <xs:enumeration value="Dissertation:phil."/>
                <xs:enumeration value="Dissertation:med."/>
                <xs:enumeration value="Dissertation:jur."/>
                <xs:enumeration value="Dissertationensammlung"/>
                <xs:enumeration value="Drama"/>
                <xs:enumeration value="Edikt"/>
                <xs:enumeration value="Einblattdruck"/>
                <xs:enumeration value="Einführung"/>
                <xs:enumeration value="Elegie"/>
                <xs:enumeration value="Emblembuch"/>
                <xs:enumeration value="Entscheidungssammlung"/>
                <xs:enumeration value="Enzyklopädie"/>
                <xs:enumeration value="Epigramm"/>
                <xs:enumeration value="Epik"/>
