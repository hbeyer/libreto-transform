                <xs:enumeration value="Epos"/>
                <xs:enumeration value="Erbauungsliteratur"/>
                <xs:enumeration value="Erlebnisbericht"/>
                <xs:enumeration value="Erotische Literatur"/>
                <xs:enumeration value="Erzählung"/>
                <xs:enumeration value="Fabel"/>
                <xs:enumeration value="Fallsammlung"/>
                <xs:enumeration value="Fastnachtsspiel"/>
                <xs:enumeration value="Fibel"/>
                <xs:enumeration value="Festbeschreibung"/>
                <xs:enumeration value="Figurengedicht"/>
                <xs:enumeration value="Flugschrift"/>
                <xs:enumeration value="Formularsammlung"/>
                <xs:enumeration value="Frauenliteratur"/>
                <xs:enumeration value="Freimaurerliteratur"/>
                <xs:enumeration value="Führer"/>
                <xs:enumeration value="Fürstenspiegel"/>
                <xs:enumeration value="Gebet"/>
                <xs:enumeration value="Gebetbuch"/>
                <xs:enumeration value="Gelegenheitsschrift"/>
