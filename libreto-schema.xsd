                <xs:enumeration value="Kalender"/>
                <xs:enumeration value="Kapitulation"/>
                <xs:enumeration value="Karte"/>
                <xs:enumeration value="Katalog"/>
                <xs:enumeration value="Katechismus"/>
                <xs:enumeration value="Kirchenlied"/>
                <xs:enumeration value="Kochbuch"/>
                <xs:enumeration value="Kolportageliteratur"/>
                <xs:enumeration value="Kommentar"/>
                <xs:enumeration value="Kommentar:jur."/>
                <xs:enumeration value="Kommentar:lit."/>
                <xs:enumeration value="Kommentar:theol."/>
                <xs:enumeration value="Kommentar:hist."/>
                <xs:enumeration value="Kommentar:polit."/>
